@model GardenArt.Models.RegisterModel
<!DOCTYPE html>
@{
    Layout = "~/Views/Layout/_LayoutTwoColumn.cshtml";
    Page.Title = "Gardenart";
}


@section TopSideNav {
    <li>Kontakta oss</li>
}

@section SideNav {
    <li><a href="~/Contact/Contact">Kontakta Gardenart</a></li>
    <li><a href="~/Wholesaler/Index">Våra återförsäljare</a></li>
}

@section Headline {

    <h3>Skapa konto</h3>

}

@section ContentLeft {

   @using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary()

    <fieldset>
        <legend>Skapa nytt användarkonto</legend>
        <ul class="side-nav">
            <li>
                @Html.LabelFor(m => m.UserName)
                @Html.TextBoxFor(m => m.UserName)
            </li>
            <li>
                @Html.LabelFor(m => m.Password)
                @Html.PasswordFor(m => m.Password)
            </li>
            <li>
                @Html.LabelFor(m => m.ConfirmPassword)
                @Html.PasswordFor(m => m.ConfirmPassword)
            </li>
        </ul>
        <input type="submit" value="Registera konto" />
    </fieldset>
}

}

@section ContentRight {

    <img src="~/Images/Site/15.png" style="margin-top:20px;" />

}


@section Scripts {
  
}
